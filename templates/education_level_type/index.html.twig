{% extends 'base.html.twig' %}

{% block title %}EducationLevelType index{% endblock %}

{% block body %}
    <h1>EducationLevelType index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Status</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for education_level_type in education_level_types %}
            <tr>
                <td>{{ education_level_type.id }}</td>
                <td>{{ education_level_type.name }}</td>
                <td>{{ education_level_type.status ? 'Yes' : 'No' }}</td>
                <td>
                    <a href="{{ path('app_education_level_type_show', {'id': education_level_type.id}) }}">show</a>
                    <a href="{{ path('app_education_level_type_edit', {'id': education_level_type.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_education_level_type_new') }}">Create new</a>
{% endblock %}
