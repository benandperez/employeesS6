{% extends 'base.html.twig' %}

{% block title %}Relationship{% endblock %}

{% block body %}
    <h1>Relationship</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ relationship.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ relationship.name }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ relationship.status ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_relationship_index') }}">back to list</a>

    <a href="{{ path('app_relationship_edit', {'id': relationship.id}) }}">edit</a>

    {{ include('relationship/_delete_form.html.twig') }}
{% endblock %}
