{% extends 'base.html.twig' %}

{% block title %}LicenseType{% endblock %}

{% block body %}
    <h1>LicenseType</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ license_type.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ license_type.name }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ license_type.status ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_license_type_index') }}">back to list</a>

    <a href="{{ path('app_license_type_edit', {'id': license_type.id}) }}">edit</a>

    {{ include('license_type/_delete_form.html.twig') }}
{% endblock %}
