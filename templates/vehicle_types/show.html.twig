{% extends 'base.html.twig' %}

{% block title %}VehicleTypes{% endblock %}

{% block body %}
    <h1>VehicleTypes</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ vehicle_type.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ vehicle_type.name }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ vehicle_type.status ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_vehicle_types_index') }}">back to list</a>

    <a href="{{ path('app_vehicle_types_edit', {'id': vehicle_type.id}) }}">edit</a>

    {{ include('vehicle_types/_delete_form.html.twig') }}
{% endblock %}
